<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.dis2013.data.Kaufvertrag" table="Kaufvertrag">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="datum" type="integer" column="Datum" />
		<property name="ort" type="string" column="Ort" />
		<property name="anzahlraten" type="integer" column="AnzahlRaten" />
		<property name="ratenzins" type="integer" column="Ratenzins" />
		
		<!-- PID fehlt noch als Fremdschluessel -->
		<!-- HausID fehlt noch als Fremdschluessel -->
		<!-- WohnungID fehlt noch als Fremdschluessel -->
		
	</class>
</hibernate-mapping>